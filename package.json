    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.0",
    "es6-promise": "^4.2.5",
    "file-loader": "^3.0.1",
    "less": "^3.9.0",
    "less-loader": "^4.1.0",
    "react-apollo": "^2.4.1",
    "react-dom": "^16.8.2",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "^4.0.8",
    "ts-loader": "^5.3.3",
    "tsconfig-paths-webpack-plugin": "^3.2.0",
